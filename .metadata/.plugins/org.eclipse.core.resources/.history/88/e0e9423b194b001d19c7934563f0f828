void printAppVer(Application* app)
{
	uint8_t major = app->version >> 24 ;
	uint8_t minor  = app->version  >> 16;
	uint8_t release =  app->version >> 8  ;
	uint8_t build = app->version ;

	printf("%d.%d.%d.%d \r\n" , major, minor, release, build );
}

