#ifndef INC_APP_H_
#define INC_APP_H_

#include "main.h"

typedef struct AppVer_{
	uint8_t major;
	uint8_t minor;
	uint8_t release;
	uint8_t build;
}AppVer;

typedef struct Application_{
	uint16_t magicNamber;
	AppVer version;
	uint8_t appType;
	uint32_t crc;
	uint8_t flag;
}Application;



void App_init(Application* app , AppVer* ver , uint8_t appType , uint8_t flag );


#endif /* INC_APP_H_ */
