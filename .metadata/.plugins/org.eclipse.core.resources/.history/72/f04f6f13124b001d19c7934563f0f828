#ifndef INC_APP_H_
#define INC_APP_H_

typedef struct Application_{
	uint16_t magicNamber;
	uint32_t version;
	uint8_t applicationType;
	uint32_t crc;
	uint8_t flag;
}Application;

void App_init(Application* app);


#endif /* INC_APP_H_ */
